{
  "version": 2,
  "builds": [
    {
      "src": "Backend/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/health", "dest": "/Backend/index.js" },
    { "src": "/api/sendmail", "dest": "/Backend/index.js", "methods": ["POST"] },
    { "src": "/(.*)", "dest": "/Backend/index.js" }
  ]
}
